{
  "extends": [
    "@vue/eslint-config-typescript",
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "warn"
  }
}